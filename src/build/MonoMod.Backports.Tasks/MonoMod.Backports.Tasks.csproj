<Project Sdk="Microsoft.NET.Sdk">
  <!-- Note: this package isn't actually referenced by anything. Instead, its files are used as inline tasks for Backports. -->
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>

    <Nullable>disable</Nullable>
    
    <IsPackable>false</IsPackable>
    <NoWarn>$(NoWarn);CA1812;CA1819;CA1031</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Build.Utilities.Core" Version="$(MSBuildRequiredVersion)" />
  </ItemGroup>

</Project>
